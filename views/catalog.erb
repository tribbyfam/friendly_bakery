

<% @catalog.each do |keys, item|%>

<section class="section section-catalog teal lighten-4">
  <div class="container">
    <div class="row">
      <div class="col s12 m4">
        <img src="<%= item.src %>" alt="" class="circle responsive-img">
      </div>
      <div class="col s12 m8">
        <h4><span class="teal-text text-darken-1"><%= item.price %> <br>
        <%= item.title %></span></h4>
        <p class="catalog"><%= item.description %></p>
      </div>
    </div>
  </div>
</section>
<% end %>