
<!DOCTYPE html>
<html>

<head>
  <!--Import Google Icon Font-->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <!--Import materialize.css-->
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
  <link type="text/css" rel="stylesheet" href="css/main.css" />
  <script defer src="https://use.fontawesome.com/releases/v5.0.6/js/all.js"></script>
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.2.0/css/all.css" integrity="sha384-hWVjflwFxL6sNzntih27bfxkr27PmbbK/iSvJ+a4+0owXq79v+lsFkW54bOGbiDQ"
    crossorigin="anonymous">
  <!--Let browser know website is optimized for mobile-->
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Custom Materialize Theme</title>
</head>

<body id="home" class="scrollspy">
  <div class="navbar-fixed">
    <nav class="green darken-3">
      <div class="container">
        <div class="nav-wrapper">
          <a href="#home" class="brand-logo center">Main St Bakery</a>
          <a href="#" data-activates="mobile-nav" class="button-collapse">
            <i class="material-icons">menu</i>
          </a>
          <ul class="right hide-on-med-and-down">
            <li>
              <a href="#home">Home</a>
            </li>
            <li>
              <a href="#search">Search</a>
            </li>
            <li>
              <a href="#popular">Popular</a>
            </li>
            <li>
              <a href="#gallery">Gallery</a>
            </li>
            <li>
              <a href="#contact">Contact</a>
            </li>
          </ul>
        </div>
      </div>
    </nav>
  </div>
  <ul class="side-nav" id="mobile-nav">
    <li>
      <a href="#home">Home</a>
    </li>
    <li>
      <a href="#search">Cookies</a>
    </li>
    <li>
      <a href="#popular">Cakes</a>
    </li>
    <li>
      <a href="#gallery">Muffins</a>
    </li>
    <li>
      <a href="#contact">Specials</a>
    </li>
  </ul>
 

  <!-- Slider Section-->
  <section class="slider">
    <ul class="slides">
      <li>
        <img src="/images/hero1.jpeg" alt="">
        <div class="caption center-align">
          <h2>We offer variety of cookies</h2>
          <h5 class="light grey-text text-lighten-3 hide-on-small-only">Made fresh daily</h5>
        </div>
      </li>
      <li>
        <img src="/images/hero3.jpeg" alt="">
        <div class="caption left-align">
          <h2>We make custom cakes</h2>
          <h5 class="light grey-text text-lighten-3 hide-on-small-only">Cakes for any occasion</h5>
        </div>
      </li>
      <li>
        <img src="/images/hero2.jpeg" alt="">
        <div class="caption right-align">
          <h2>Cupcakes, Muffins and more</h2>
          <h5 class="light grey-text text-lighten-3 hide-on-small-only">One stop for all your pastry needs</h5>
        </div>
      </li>
    </ul>
  </section>

 <%= yield %>

  <!-- Footer -->

  <footer class="section teal darken-2 white-text center">
    <p class="flow-text">bakery &copy; 2018</p>
  </footer>


  <!--Import jQuery before materialize.js-->
  <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
  <script type="text/javascript" src="js/materialize.min.js"></script>
  <script src="main.js"></script>
  <script>
    M.AutoInit();
    $(document).ready(function () {
      $('.button-collapse').sideNav();

    });
    document.addEventListener('DOMContentLoaded', function () {
      var elems = document.querySelectorAll('.carousel');
      var instances = M.Carousel.init(elems, 'duration', 2000);
    });

    document.addEventListener('DOMContentLoaded', function () {
      var elems = document.querySelectorAll('.slider');
      var instances = M.Slider.init(elems, 'height: 400');
    });
    $('.autocomplete').autocomplete({
      data: {
        "Aruba": null,
        "Cancun Mexico": null,
        "Hawaii": null,
        "California": null,
      }
    });

    document.addEventListener('DOMContentLoaded', function () {
      var elems = document.querySelectorAll('.scrollspy');
      var instances = M.ScrollSpy.init(elems, options);
    });

    //init sidenav
    //init slider
  </script>
</body>

</html>